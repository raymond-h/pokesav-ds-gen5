{
  "name": "pokesav-ds-gen5",
  "description": "Pokemon savefile reader for DS (Generation 5) games",
  "version": "0.1.0",
  "author": "Raymond Hammarling",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint lib/ test/",
    "test": "ava",
    "watch": "ava -w",
    "update-test-snapshots": "ava --update-snapshots",
    "build:kaitai": "node scripts/build-kaitai-format.js -d formats-compiled/ -s formats/**/*.ksy",
    "watch:kaitai": "onchange formats/ -- npm run build:kaitai",
    "build": "npm run build:kaitai",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^4.18.2",
    "eslint-config-kellyirc": "^3.1.0",
    "falafel": "^2.1.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "js-yaml": "^3.13.1",
    "jsverify": "^0.8.3",
    "kaitai-struct-compiler": "^0.8.0-SNAPSHOT.20180205.11616",
    "minimist": "^1.2.3",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0"
  },
  "dependencies": {
    "crc": "^4.1.1",
    "kaitai-struct": "^0.8.0-SNAPSHOT.2",
    "lodash.chunk": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raymond-h/pokesav-ds-gen5"
  }
}
